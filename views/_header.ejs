<header>
    <div id="header">
        <span id="title"><span id="connect">Connect</span>&nbsp;<div id="span"> Us</div></span>
        <!-- <img src="/Logo/output-onlinepngtools (1).png" alt="Connect-Us"> -->
        <ul id="list">
            <% if (locals.user) { %>
                    <a id="link" href="/users/profile/<%= user.id%>"><%= user.name %></a> &nbsp;
                    <a id="link" href="/users/sign-out">Sign Out</a>
                
            <% }else{ %>
                    <a id="link" href="/users/sign-in">Sign In</a>&nbsp;    
                    <a id="link" href="/users/sign-up">Sign Up</a>
            <% } %>    
        </ul>
    </div>
    
</header>